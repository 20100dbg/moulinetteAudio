set dossier_in=in
set dossier_out=out

rem Nombre de canaux : Mono = 1 , Stereo = 2
set ac=1

rem Echantillonage (En Hertz, valeurs possibles : 11025, 22050, 44100, 48000)
set ar=22050

rem Debit (en Kbps, valeurs possibles : 32, 64, 96, 128, 192, 256, 320)
set ab=128

rem format de sortie : mp3, wav
set codec=mp3
